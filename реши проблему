meme_dict = {
            "КРИНЖ": "Что-то очень стыдное или странное",
            "ЛОЛ": "Что-то очень смешное, как АЙМАО",
            "АЙМАО": "Что-то очень смешное, это как ЛОЛ",
            "РОФЛ": "Шутка",
            "КРИПОВЫЙ": "Страшный или пугающий",
            "АГРИТЬСЯ": "Злиться"
            }
            
while True:
    word = input("Введите какой-то слэнг(БОЛЬШИМИ БУКВАМИ)")
    
    if word in meme_dict.keys():
        print(meme_dict[word])
    else:
        print("НЕТУ, ПРОСТО ЗДЕСЬ НЕТУ")
